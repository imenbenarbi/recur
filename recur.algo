n!=n*(n-1)!
FUNCTION fact(n) : INTEGER
VAR
    rslt:INTEGER
BEGIN
     if(n=0)then
     reslt:=1

     ELSE
       rslt:=n* fact(n-1)
     END_IF
    RETURN rslt ;
END
